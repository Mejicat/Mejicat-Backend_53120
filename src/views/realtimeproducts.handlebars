<!-- Formulario para ingresar un nuevo producto -->

<form id="addProductForm">
  <input type="text" id="title" name="title" placeholder="Title" required />
  <input type="text" id="description" name="description" placeholder="Description" required />
  <input type="number" id="price" name="price" placeholder="Price" required />
  <input type="text" id="thumbnails" name="thumbnails" placeholder="Thumbnails URL"/>
  <input type="text" id="code" name="code" placeholder="Product Code" required/>
  <input type="number" id="stock" name="stock" placeholder="Stock" required/>
  <button type="button" onclick="addProduct()">Agregar Producto</button>
</form>

<div id="products-container" class="product-grid">
</div>

